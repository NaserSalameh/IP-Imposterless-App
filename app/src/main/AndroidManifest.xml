<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nasersalameh.imposterphenomenoninterventionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImposterPhenomenonInterventionApp">
        <activity
            android:name=".activities.main.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.ImposterPhenomenonInterventionApp.NoActionBar"></activity>
        <activity android:name=".activities.setup.SetupActivity" />
        <activity android:name=".activities.startup.StartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>