<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="10">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout11"
                android:layout_width="match_parent"
                android:layout_height="242dp">

                <TextView
                    android:id="@+id/cipsScoreTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="76dp"
                    android:layout_marginTop="68dp"
                    android:text="TextView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/cipsResultTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="76dp"
                    android:layout_marginTop="25dp"
                    android:text="TextView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cipsScoreTextView" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/divider9"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout10"
                android:layout_width="match_parent"
                android:layout_height="280dp">

                <ProgressBar
                    android:id="@+id/perfectionismScaleBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="54dp"
                    android:layout_height="146dp"
                    android:layout_marginStart="72dp"
                    android:layout_marginTop="32dp"
                    android:rotation="-90"
                    android:scaleX="2.5"
                    android:scaleY="5"
                    app:layout_constraintStart_toEndOf="@+id/achievementScaleBar"
                    app:layout_constraintTop_toBottomOf="@+id/textView4" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="108dp"
                    android:layout_height="42dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="20dp"
                    android:text="Perfectionism"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/textView11"
                    app:layout_constraintTop_toBottomOf="@+id/perfectionismScaleBar" />

                <ProgressBar
                    android:id="@+id/achievementScaleBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="54dp"
                    android:layout_height="146dp"
                    android:layout_marginStart="72dp"
                    android:layout_marginTop="32dp"
                    android:rotation="-90"
                    android:scaleX="2.5"
                    android:scaleY="5"
                    app:layout_constraintStart_toEndOf="@+id/abilityScaleBar"
                    app:layout_constraintTop_toBottomOf="@+id/textView4" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="108dp"
                    android:layout_height="42dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="8dp"
                    android:text="Discounting Achievements"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/textView10"
                    app:layout_constraintTop_toBottomOf="@+id/achievementScaleBar" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="75dp"
                    android:layout_marginTop="16dp"
                    android:text="These behaviours have been detected:"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ProgressBar
                    android:id="@+id/abilityScaleBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="54dp"
                    android:layout_height="146dp"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="32dp"
                    android:rotation="-90"
                    android:scaleX="2.5"
                    android:scaleY="5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView4" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="108dp"
                    android:layout_height="42dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="8dp"
                    android:text="Underestimating Abilities"
                    android:textAlignment="center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/abilityScaleBar" />

                <TextView
                    android:id="@+id/abilityScoreTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="50dp"
                    android:layout_marginBottom="4dp"
                    android:text="TextView"
                    app:layout_constraintBottom_toTopOf="@+id/abilityScaleBar"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/achievementScoreTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="68dp"
                    android:layout_marginBottom="4dp"
                    android:text="TextView"
                    app:layout_constraintBottom_toTopOf="@+id/achievementScaleBar"
                    app:layout_constraintStart_toEndOf="@+id/abilityScoreTextView" />

                <TextView
                    android:id="@+id/perfectionismScoreTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="68dp"
                    android:layout_marginBottom="4dp"
                    android:text="TextView"
                    app:layout_constraintBottom_toTopOf="@+id/perfectionismScaleBar"
                    app:layout_constraintStart_toEndOf="@+id/achievementScoreTextView" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/divider8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/setupResultsButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="96dp"
                    android:layout_marginTop="44dp"
                    android:text="@string/setup_results_button"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>