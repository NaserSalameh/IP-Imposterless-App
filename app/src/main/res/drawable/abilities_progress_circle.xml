<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="false"
            >
            <solid android:color="#DDD"></solid>

        </shape>
    </item>
    <item  android:id="@+id/progressColor">
        <rotate
            android:fromDegrees="270"
            android:toDegrees="270">
        <shape
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="true"
            >
<!--            <solid android:color="@color/dark_blue_background"></solid>-->
            <gradient
                android:startColor="@color/help_icons_fg"
                android:endColor="@color/primary_color"
                ></gradient>
        </shape>
        </rotate>
    </item>
</layer-list>